class Solution {
    public boolean isPalindrome(int x) {
        int o=x;
        int rev=0;
        while(x>0){
            int rem=x%10;
            rev=rev*10+rem;
            x=x/10;

        }
        if(rev==o){
            return true;
        }
        else{
            return false;
        }
        
    }
    public static void main(String args[]){
        int x=Integer.parseInt(args[0]);
        Solution su=new Solution();
        su.isPalindrome(x);

    }
}
